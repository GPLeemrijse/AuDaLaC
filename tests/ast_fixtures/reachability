struct Node (reachable : Bool) {
    init {
        Node s1 := Node(true);
        Node n1 := Node(false);
        Node n2 := Node(false);
        Node n3 := Node(false);
        Edge e1 := Edge(s1, n2);
        Edge e2 := Edge(n3, n2);
        Edge e3 := Edge(n1, n3);
        Edge e4 := Edge(n2, n3);
    }
}

struct Edge (n1 : Node, n2 : Node) {
    reachability {
        if n1.reachable then {
            n2.reachable := true;
        }
    }
}

init < Fix(reachability)
